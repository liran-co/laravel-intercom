{
    "name": "liran-co/laravel-intercom",
    "description": "Laravel wrapper for Intercom PHP.",
    "keywords": [
        "laravel",
        "intercom",
        "laravel-intercom"
    ],
    "homepage": "https://github.com/liran-co/laravel-intercom",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Liran Cohen",
            "email": "l@liran.co"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "intercom/intercom-php": "^4.4",
        "symfony/http-client": "^6.3|^7.0",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "LiranCo\\LaravelIntercom\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiranCo\\LaravelIntercom\\LaravelIntercomServiceProvider"
            ],
            "aliases": {
                "Intercom": "LiranCo\\LaravelIntercom\\LaravelIntercomFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
